{
  "expo": {
    "name": "Remynd",
    "slug": "Remynd",
    "version": "1.0.0",
    "orientation": "portrait",
    "experiments": {
      "tsconfigPaths": true
    },
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0397FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0000ff"
      },
      "package": "com.osval1.Remynd"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "099716eb-08df-4851-855d-4e03549c1423"
      }
    }
  }
}
